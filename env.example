# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
TIME_ZONE=Asia/Ho_Chi_Minh

# MQTT Settings
MQTT_BROKER=broker.emqx.io
MQTT_PORT=1883
MQTT_CLIENT_ID=Django_CatCare_Server
MQTT_USERNAME=
MQTT_PASSWORD=

# MQTT Topics
MQTT_TOPIC_FEED=catcare/feed
MQTT_TOPIC_MODE=catcare/mode
MQTT_TOPIC_STATUS=catcare/status
MQTT_TOPIC_IMAGE=catcare/image
MQTT_TOPIC_FEED_LOG=catcare/feed_log
MQTT_TOPIC_IMAGE_CHUNK=catcare/image_chunk
MQTT_TOPIC_DETECTION=catcare/detection
MQTT_TOPIC_CAMERA_CONTROL=catcare/camera_control

# Camera Settings - Đặt True để flip camera nếu ESP32 bị quay ngược
CAMERA_FLIP_HORIZONTAL=True
CAMERA_FLIP_VERTICAL=False
CAMERA_ROTATE_180=False
CAMERA_DETECTION_ENABLED=True
CAMERA_DETECTION_INTERVAL=5
DETECTION_CONFIDENCE_THRESHOLD=0.5

# ESP32 Settings - Thay bằng IP thật của ESP32
ESP32_IP=192.168.100.141